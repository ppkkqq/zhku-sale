{
  "name": "nuxt-element-dashboard",
  "version": "2.1.0",
  "description": "Nuxt2 + element UI project",
  "author": "levy <levy9527@qq.com>",
  "license": "MIT",
  "keywords": ["vue", "element-ui", "nuxt", "dashboard"],
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.2.0"
  },
  "scripts": {
    "mock": "cross-env mode=mock IS_PROXY=1 nuxt",
    "dev": "cross-env HOST=0.0.0.0 mode=dev IS_PROXY=1 nuxt",
    "test":
      "cross-env HOST=0.0.0.0 mode=test GATEWAY=http://malltest.gtytong.com IS_PROXY=1 nuxt",
    "mock:nologin":
      "cross-env HOST=0.0.0.0 mode=mock IS_PROXY=1 NO_LOGIN=1 nuxt",
    "dev:nologin": "cross-env mode=dev IS_PROXY=1 NO_LOGIN=1 nuxt",
    "test:e2e": "nuxt-puppeteer-jest",
    "build": "cross-env mode=prod nuxt build",
    "build:docker": "nuxt build",
    "start": "cross-env HOST=0.0.0.0 PORT=3333 mode=prod nuxt start",
    "start:docker": "nuxt start",
    "generate": "cross-env mode=prod nuxt generate",
    "deploy": "now --npm --public",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "@xkeshi/vue-countdown": "^1.0.1",
    "ali-oss": "^6.0.1",
    "dayjs": "^1.7.4",
    "dotenv": "^6.1.0",
    "el-data-table": "^0.4.1",
    "el-form-renderer": "^1.1.0",
    "element-ui": "^2.4.8",
    "js-cookie": "^2.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "nuxt-edge": "^2.0.0-25428457.c59acdd",
    "promise.prototype.finally": "^3.1.0",
    "qrcodejs2": "^0.0.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "v-distpicker": "^1.0.20",
    "vant": "^1.4.2",
    "vee-validate": "^2.1.0-beta.11",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.3.3",
    "@nuxtjs/proxy": "^1.2.4",
    "babel-eslint": "^8.2.1",
    "babel-plugin-component": "^1.1.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^0.14.3",
    "jsdom": "^11.10.0",
    "prettier": "1.12.1",
    "pretty-quick": "^1.4.1"
  }
